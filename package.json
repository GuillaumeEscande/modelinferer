{
  "name": "modelinferer",
  "version": "1.0.0",
  "description": "Tools to generate js model based on yaml descriptor and infer link between objects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "^8.2.0",
    "@types/node": "14.0.13",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "js-yaml": "^3.14.1",
    "mocha": "^8.2.1",
    "node": "^15.4.0",
    "nyc": "^15.1.0",
    "ts-node": "8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "dev": "_mocha --require ts-node/register --watch-extensions ts --watch --recursive src/**/*.spec.* test/**/*.spec.*",
    "lint": "eslint src/**/*.ts test/**/*.ts && tslint src/**/*.ts test/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts test/**/*.ts && tslint --fix src/**/*.ts test/**/*.ts",
    "test": "_mocha --require ts-node/register --recursive src/**/*.spec.* test/**/*.spec.*",
    "test-u": "_mocha --require ts-node/register --recursive src/**/*.spec.*",
    "test-i": "_mocha --require ts-node/register --recursive test/**/*.spec.*",
    "coverage": "nyc --reporter=html --reporter=text _mocha --require ts-node/register --recursive src/**/*.spec.* test/**/*.spec.*",
    "clean": "rm -rf .nyc_output coverage build",
    "build": "npm run clean; npm run lint && npm run test",
    "pub": "git push && npm run build && npm-bump patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuillaumeEscande/modelinferer.git"
  },
  "author": "EScande Guillaume",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GuillaumeEscande/modelinferer/issues"
  },
  "homepage": "https://github.com/GuillaumeEscande/modelinferer#readme"
}
